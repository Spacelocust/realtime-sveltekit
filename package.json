    "lucide-svelte": "^0.336.0",
    "mode-watcher": "^0.2.1",
    "mysql2": "^3.9.1",
    "oslo": "^1.1.2",
    "shadcn-svelte": "^0.7.0",
    "svelte-sonner": "^0.3.18",
    "tailwind-merge": "^2.2.1",
    "tailwind-variants": "^0.2.0",
    "valibot": "^0.28.1"
