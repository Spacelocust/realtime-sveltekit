{
  "name": "cleancode-sveltekit",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test": "playwright test",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint .",
    "format": "eslint . --fix",
    "db:migrate": "bun --bun run ./src/lib/server/drizzle/migrate.ts",
    "db:drop": "bun --bun run ./src/lib/server/drizzle/drop.ts",
    "db:create": "bun --bun run ./src/lib/server/drizzle/create.ts"
  },
  "devDependencies": {
    "@playwright/test": "^1.41.2",
    "@sveltejs/kit": "^2.5.0",
    "@types/node": "^20.11.19",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "autoprefixer": "^10.4.17",
    "bun-types": "^1.0.27",
    "drizzle-kit": "^0.20.14",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-import-no-duplicates-prefix-resolved-path": "^2.0.0",
    "eslint-plugin-jsdoc": "^48.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-svelte": "^2.35.1",
    "lucia": "^3.0.1",
    "postcss": "^8.4.35",
    "postcss-load-config": "^5.0.3",
    "prettier": "^3.2.5",
    "prettier-plugin-svelte": "^3.2.1",
    "prettier-plugin-tailwindcss": "0.4.1",
    "svelte": "^4.2.11",
    "svelte-adapter-bun": "^0.5.1",
    "svelte-check": "^3.6.4",
    "sveltekit-superforms": "^2.5.0",
    "tailwindcss": "^3.4.1",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vite": "^5.1.3"
